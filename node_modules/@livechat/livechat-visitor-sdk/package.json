{
  "name": "@livechat/livechat-visitor-sdk",
  "version": "0.28.4",
  "description": "LiveChat visitor JS SDK to perform a chat as a visitor",
  "contributors": [
    "Konrad Kruk <k.kruk@livechatinc.com>",
    "Mateusz Burzy≈Ñski <m.burzynski@livechatinc.com>"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "keywords": [
    "livechat"
  ],
  "dependencies": {
    "@livechat/redux-utils": "^0.0.0",
    "@livechat/saga-utils": "^0.0.1",
    "@livechat/cookies": "^0.0.1",
    "lodash": "^4.17.4",
    "memory-web-storage": "^0.1.0",
    "mitt": "^1.1.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.6",
    "regenerator-runtime": "^0.10.5",
    "unfetch": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-loader": "^7.1.2",
    "cross-env": "^5.0.0",
    "http-server": "^0.10.0",
    "jest": "^21.2.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.51.7",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "scripts": {
    "prebuild": "rimraf es lib dist",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -i src/index.js -o dist/livechat-visitor-sdk.min.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -i src/index.js -o dist/livechat-visitor-sdk.js",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --ignore *.test.js,__mocks__,types --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --ignore *.test.js,__mocks__,types --out-dir es",
    "build": "run-p build:**",
    "watch": "npm run build:es -- --watch",
    "prepare": "npm run build",
    "prepublish": "test $(npm -v | tr . '\\n' | head -n 1) -ge '4' || exit 1",
    "dev": "webpack-dev-server -d --config ./webpack.config.js --port 3074",
    "precommit": "cat website-docs/_introduction.md website-docs/_how_to_start.md website-docs/_methods.md website-docs/_callbacks.md website-docs/_changelog.md > Readme.md && git --git-dir ../../.git --work-tree ../../ add Readme.md",
    "test": "jest"
  }
}
